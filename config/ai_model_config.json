{
  "ai_models": {
    "traffic_analysis": {
      "model_type": "neural_network",
      "parameters": {
        "layers": [64, 32, 16],
        "activation": "relu",
        "optimizer": "adam",
        "loss_function": "mean_squared_error"
      },
      "training": {
        "batch_size": 32,
        "epochs": 100,
        "validation_split": 0.2
      }
    },
    "anomaly_detection": {
      "model_type": "autoencoder",
      "parameters": {
        "encoding_dim": 32,
        "activation": "sigmoid"
      },
      "training": {
        "batch_size": 64,
        "epochs": 50,
        "early_stopping": true
      }
    },
    "user_behavior_modeling": {
      "model_type": "decision_tree",
      "parameters": {
        "max_depth": 5,
        "min_samples_split": 10
      },
      "training": {
        "cross_validation": 5
      }
    }
  }
}
